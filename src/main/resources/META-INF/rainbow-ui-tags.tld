    <tag>
        <name>combotree</name>
        <tagclass>com.linkage.rainbow.ui.views.jsp.ui.combotree.ComboTreeTag</tagclass>
        <bodycontent>JSP</bodycontent>
        <info>树标签</info>
        <attribute>
            <name>id</name>       <!-- 树的div id -->
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[树的id，表示每个树的唯一标识，也是装载树的div的id]]></description>
		</attribute>
        <attribute>
            <name>width</name>    <!-- 树的div宽度 -->
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[树的宽度]]></description>
		</attribute>
        <attribute>
            <name>height</name>   <!-- 树的div高度 -->
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[树的高度]]></description>
		</attribute>
        <attribute>
            <name>template</name> <!-- 模板 -->
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[菜单树的模式:导航树，带radio的导航树，带checkbox的选择。参数为:simple,radio,checkbox]]></description>
		</attribute>
        <attribute>
            <name>style</name>     <!-- 树的样式 -->
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[加载树的div的样式]]></description>
		</attribute>
        <attribute>
            <name>method</name>    <!-- 树调用的方法 -->
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[生成树的spring方法，格式如下:在spring定义中的bean的id+”.”+方法名。方法中的参数为HashMap
				如:demo-module-test-treeFacade.selectOnceLinkTree
			]]></description>
		</attribute>
        <attribute>
            <name>params</name>      <!-- 树的参数 -->
            <required>true</required>
            <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[方法定义的参数由字符串组成,参数名:值;
				如: super_id:0;name:bin;
