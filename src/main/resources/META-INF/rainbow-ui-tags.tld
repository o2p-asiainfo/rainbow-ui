     <!-- easyui标签页 --> 
    <tag>
	<name>tabpage</name>
    <tagclass>com.linkage.rainbow.ui.views.jsp.ui.tabpage.TabPageTag</tagclass>
    <bodycontent>JSP</bodycontent>
    <info>标签页</info>
    <!--公共属性名 -->
	<attribute>
		<name>theme</name><!-- 主题 -->
		<required>false</required>
		<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[]]></description>
		</attribute>
	<attribute>
		<name>template</name><!-- 模板名称 -->
		<required>false</required>
		<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[模板名称(默认simple 目前仅该模板)]]></description>
		</attribute>
	<attribute>
		<name>skin</name><!-- 组件皮肤 -->
		<required>false</required>
		<rtexprvalue>true</rtexprvalue>
		<description><![CDATA[不同皮肤的选择(默认default) 目前提供：default]]></description>
	</attribute>
	<!--公共属性名end -->
    <attribute>
	    <name>id</name> <!-- 标签页对应的id -->
	    <required>true</required>
	    <rtexprvalue>true</rtexprvalue>
		<description><![CDATA[标签页对应的id。
				通过此ID，$('#id').tabs('add',{
				title:'New Tab '
				herf:'a.shtml',
				iconCls:'icon-save',
				closable:true
			}来动态增加标签页
			]]></description>
	</attribute>
    <attribute>
	    <name>height</name> <!-- 标签页整体的高度 -->
	    <required>false</required>
	    <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[标签页整体的高度]]></description>
		</attribute>
    <attribute>
