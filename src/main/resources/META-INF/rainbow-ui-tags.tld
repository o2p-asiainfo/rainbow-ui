     <tag>
		<name>combobox</name>
		<tagclass>com.linkage.rainbow.ui.views.jsp.ui.comboBox.ComboboxTag</tagclass>
		<bodycontent>JSP</bodycontent>
		<info>布局标签</info>
		<!--公共属性名 -->
		<attribute>
			<name>name</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
			<description>表单名,与action中的属性绑定</description>
		</attribute>
		<attribute>
			<name>id</name><!-- ID -->
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[表单ID]]></description>
		</attribute>
		<attribute>
			<name>value</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>默认选中项用，多个值用，隔开，如果未设置，会自动根据name中设置的值从action中get相应的值</description>
		</attribute>
		<attribute>
			<name>theme</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[主题,JSP页面中不设置，由默认值决定]]></description>
		</attribute>
		<attribute>
			<name>template</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[模板名称，JSP页面中不设置，由默认值决定]]></description>
		</attribute>
		<attribute>
			<name>skin</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[组件皮肤，JSP页面中不设置，由默认值决定与session中设置的皮肤为准,如果有指定则按指定的皮肤显示]]></description>
		</attribute>
		<!--公共属性名end -->
		<attribute>
            <name>width</name>    
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[宽度]]></description>
		</attribute>
        <attribute>
            <name>height</name>  
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[高度]]></description>
		</attribute>
		<attribute>
			<name>valueField</name><!-- ID -->
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description><![CDATA[下拉框ID值字段名，默认值为：id]]></description>
		</attribute>
		<attribute>
			<name>textField</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>下拉框内容字段名，默认值为：text</description>
		</attribute>
		<attribute>
			<name>multiple</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>是否支持多选,true 或false</description>
		</attribute>		
		<attribute>
			<name>panelWidth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>panel宽度</description>
		</attribute>
		<attribute>
			<name>panelHeight</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
			<description>panel高度,按下拉的项目自动扩展为auto</description>
		</attribute>
	    <attribute>
            <name>method</name>
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[生成可选项的spring方法，格式如下:在spring定义中的bean的id+”.”+方法名。方法中的参数为HashMap
				如:demo-module-test-treeFacade.selectOnceLinkTree,此方法入参为HashMap或Map,出参为List，List内可以为Map,或javabean
			]]></description>
		</attribute>
        <attribute>
            <name>params</name>      
            <required>false</required>
            <rtexprvalue>true</rtexprvalue>
			<description><![CDATA[方法定义的参数由字符串组成,参数名:值;
				如: super_id:0;name:bin;
			]]></description>
		</attribute>		
	</tag>
    
    
